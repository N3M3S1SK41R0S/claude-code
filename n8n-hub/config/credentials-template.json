{
  "_NEMESIS_HUB_": "Configuration Complete v2.0",
  "_VERSION_": "2.0.0",
  "_INSTRUCTIONS_": [
    "1. Copiez ce fichier vers 'credentials.json'",
    "2. Remplacez toutes les valeurs VOTRE_* par vos vraies cles",
    "3. Importez dans n8n: Settings > Credentials",
    "4. Ne partagez JAMAIS ce fichier avec vos vraies cles"
  ],

  "platforms": {
    "n8n": {
      "name": "n8n (local)",
      "api_url": "http://localhost:5678/api/v1",
      "api_key": "VOTRE_CLE_API_N8N",
      "docs": "http://localhost:5678/settings",
      "instructions": [
        "1. Ouvrez http://localhost:5678",
        "2. Settings > API > Enable API",
        "3. Create API Key et copiez-la"
      ]
    },

    "google_ai_studio": {
      "name": "Google AI Studio / Gemini",
      "api_key": "VOTRE_CLE_API_GOOGLE_AI",
      "docs": "https://aistudio.google.com/app/apikey"
    },

    "notion": {
      "name": "Notion",
      "api_key": "secret_VOTRE_CLE_NOTION",
      "database_ids": {
        "main": "VOTRE_DATABASE_ID_PRINCIPAL",
        "sync_log": "VOTRE_SYNC_LOG_DB_ID",
        "content": "VOTRE_CONTENT_DB_ID",
        "social_log": "VOTRE_SOCIAL_LOG_DB_ID",
        "health": "VOTRE_HEALTH_DB_ID",
        "error_log": "VOTRE_ERROR_LOG_DB_ID",
        "backup": "VOTRE_BACKUP_DB_ID",
        "data_source": "VOTRE_DATA_SOURCE_DB_ID"
      },
      "docs": "https://www.notion.so/my-integrations"
    },

    "discord": {
      "name": "Discord Bot",
      "bot_token": "VOTRE_TOKEN_BOT_DISCORD",
      "channels": {
        "log": "ID_CHANNEL_LOG",
        "alert": "ID_CHANNEL_ALERT",
        "error": "ID_CHANNEL_ERROR",
        "content": "ID_CHANNEL_CONTENT",
        "social": "ID_CHANNEL_SOCIAL",
        "data": "ID_CHANNEL_DATA"
      },
      "docs": "https://discord.com/developers/applications"
    },

    "github": {
      "name": "GitHub",
      "personal_access_token": "ghp_VOTRE_TOKEN_GITHUB",
      "owner": "VOTRE_USERNAME_OU_ORG",
      "default_repo": "VOTRE_REPO_PRINCIPAL",
      "docs": "https://github.com/settings/tokens"
    },

    "heygen": {
      "name": "HeyGen",
      "api_key": "VOTRE_CLE_API_HEYGEN",
      "docs": "https://app.heygen.com/settings"
    },

    "midjourney": {
      "name": "Midjourney (via GoAPI)",
      "goapi_key": "VOTRE_CLE_GOAPI",
      "discord_channel_id": "ID_CHANNEL_MIDJOURNEY",
      "docs": "https://www.goapi.ai"
    },

    "bubble": {
      "name": "Bubble.io",
      "api_key": "VOTRE_CLE_API_BUBBLE",
      "app_name": "VOTRE_APP_NAME",
      "docs": "https://bubble.io/reference"
    },

    "make": {
      "name": "Make (Integromat)",
      "api_key": "VOTRE_CLE_API_MAKE",
      "webhook_url": "https://hook.eu1.make.com/VOTRE_WEBHOOK_ID",
      "docs": "https://www.make.com/en/api-documentation"
    },

    "slack": {
      "name": "Slack",
      "token": "xoxb-VOTRE_TOKEN_SLACK",
      "channel": "ID_CHANNEL_SLACK",
      "docs": "https://api.slack.com/apps"
    }
  },

  "environment_variables": {
    "_comment_": "Copier dans le fichier .env",

    "N8N_API_URL": "http://localhost:5678/api/v1",
    "N8N_API_KEY": "VOTRE_CLE_API_N8N",

    "GOOGLE_AI_API_KEY": "VOTRE_CLE_GOOGLE_AI",

    "NOTION_API_KEY": "secret_VOTRE_CLE_NOTION",
    "NOTION_DATABASE_ID": "VOTRE_DATABASE_ID",
    "NOTION_SYNC_LOG_DB": "ID_SYNC_LOG",
    "NOTION_CONTENT_DB": "ID_CONTENT",
    "NOTION_SOCIAL_LOG_DB": "ID_SOCIAL_LOG",
    "NOTION_HEALTH_DB": "ID_HEALTH",
    "NOTION_ERROR_LOG_DB": "ID_ERROR_LOG",
    "NOTION_BACKUP_DB": "ID_BACKUP",
    "NOTION_DATA_SOURCE_DB": "ID_DATA_SOURCE",

    "DISCORD_BOT_TOKEN": "VOTRE_TOKEN_DISCORD",
    "DISCORD_LOG_CHANNEL": "ID_LOG",
    "DISCORD_ALERT_CHANNEL": "ID_ALERT",
    "DISCORD_ERROR_CHANNEL": "ID_ERROR",
    "DISCORD_CONTENT_CHANNEL": "ID_CONTENT",
    "DISCORD_SOCIAL_CHANNEL": "ID_SOCIAL",
    "DISCORD_DATA_CHANNEL": "ID_DATA",

    "GITHUB_TOKEN": "ghp_VOTRE_TOKEN",
    "GITHUB_OWNER": "VOTRE_USERNAME",
    "GITHUB_REPO": "VOTRE_REPO",

    "HEYGEN_API_KEY": "VOTRE_CLE_HEYGEN",

    "GOAPI_KEY": "VOTRE_CLE_GOAPI",
    "MJ_CHANNEL_ID": "ID_CHANNEL_MJ",

    "BUBBLE_API_KEY": "VOTRE_CLE_BUBBLE",
    "BUBBLE_APP_NAME": "VOTRE_APP",

    "MAKE_API_KEY": "VOTRE_CLE_MAKE",
    "MAKE_WEBHOOK_URL": "https://hook.eu1.make.com/VOTRE_WEBHOOK",

    "SLACK_TOKEN": "xoxb-VOTRE_TOKEN",
    "SLACK_SOCIAL_CHANNEL": "ID_CHANNEL"
  },

  "webhooks_map": {
    "_comment_": "Tous les webhooks disponibles - Base: http://localhost:5678/webhook/",

    "orchestrator": "Point d'entree principal",
    "nemesis-hub-master": "Hub central",
    "dashboard": "Dashboard monitoring",
    "unified-sync": "Sync toutes plateformes",
    "backup": "Backup automatique",
    "connection-test": "Test connexions API",
    "error-handler": "Gestionnaire erreurs",
    "relay": "Relay webhook universel",
    "mcp-bridge": "Bridge Claude MCP",
    "etl-pipeline": "Pipeline ETL",
    "ai-content": "Generation IA",
    "social-post": "Publication social",
    "notion-sync": "Sync Notion",
    "discord-bot": "Bot Discord",
    "github-events": "Events GitHub",
    "heygen-video": "Video HeyGen",
    "midjourney-gen": "Image Midjourney",
    "bubble-connector": "Connector Bubble",
    "make-zapier-in": "Bridge Make/Zapier",
    "google-ai-studio": "API Gemini"
  },

  "n8n_credentials_import": {
    "credentials": [
      { "name": "n8n API", "type": "httpHeaderAuth", "data": { "name": "X-N8N-API-KEY", "value": "VOTRE_CLE" } },
      { "name": "Google AI", "type": "httpQueryAuth", "data": { "name": "key", "value": "VOTRE_CLE" } },
      { "name": "Notion", "type": "notionApi", "data": { "apiKey": "secret_VOTRE_CLE" } },
      { "name": "Discord", "type": "discordBotApi", "data": { "botToken": "VOTRE_TOKEN" } },
      { "name": "GitHub", "type": "githubApi", "data": { "accessToken": "ghp_VOTRE_TOKEN" } },
      { "name": "HeyGen", "type": "httpHeaderAuth", "data": { "name": "X-Api-Key", "value": "VOTRE_CLE" } },
      { "name": "GoAPI", "type": "httpHeaderAuth", "data": { "name": "X-API-KEY", "value": "VOTRE_CLE" } },
      { "name": "Bubble", "type": "httpHeaderAuth", "data": { "name": "Authorization", "value": "Bearer VOTRE_CLE" } },
      { "name": "Slack", "type": "slackApi", "data": { "accessToken": "xoxb-VOTRE_TOKEN" } }
    ]
  }
}
